<!-- views/index.ejs -->
<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
</head>
<body>
    <h1><%= title %></h1>
    
    <h2>User List:</h2>
    <ul>
        <% users.forEach(user => { %>
            <li>
                Name: <%= user.name %>, Age: <%= user.age %>
            </li>
        <% }); %>
    </ul>
</body>
</html>